{

  "ConnectionStrings": {
    "SimplifyDatabase": "Server=${DB_SERVER};Database=Simplify;User Id=${DB_USER};Password=${DB_PASSWORD};TrustServerCertificate=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JWT": {
    "ValidAudience": "${JWT_AUDIENCE}",
    "ValidIssuer": "${JWT_ISSUER}",
    "Secret": "${JWT_SECRET}"
  },
  "AllowedHosts": "*",
  "NotificationSettings": {
    "UseTestingNotificationSender": false
  }
}
